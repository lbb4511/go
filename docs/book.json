{
    "gitbook": "3.2.3",
    "title": "Go语言编程笔记",
    "description": "探索学习发现Go语言",
    "author": "lbb",
    "language": "zh-hans",
    "plugins": [
        "-lunr",
        "-search",
        "-sharing",
        "search-plus",
        "anchor-navigation-ex",
        "expandable-chapters-small"
    ],
    "pluginsConfig": {
        "fontSettings": {
            "theme": "white",
            "family": "sans",
            "size": 2
        },
        "anchor-navigation-ex": {
            "associatedWithSummary": false,
            "showLevel": true,
            "multipleH1": false,
            "mode": "float",
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        }
    }
}